###
### File managed by Puppet
###
NETWORKING=yes
NETWORKING_IPV6=no
HOSTNAME=<%= fqdn %>
<% if !gateway.empty? %>GATEWAY=<%= gateway %>
<% end -%>
<% if !gatewaydev.empty? %>GATEWAYDEV=<%= gatewaydev %>
<% end -%>
<% if !nisdomain.empty? %>NISDOMAIN=<%= nisdomain %>
<% end -%>
<% if !vlan.empty? %>VLAN=<%= vlan %>
<% end -%>
<% if !nozeroconf.empty? %>NOZEROCONF=<%= nozeroconf %>
<% end -%>
